//create Calculator class
class Calcoolator {
    //Method to add two numbers
    public int add(int a, int b) {
        return a + b;
    }
    //Method to subtract two numbers
    public int sub(int a, int b) {
        return a - b;
    }
    //Method to divide to numbers
    public int div(int a, int b) {
        return a / b;
    }
    //Method to multiply two numbeers
    public int mul(int a, int b) {
        return a * b;
    }
}
//create Calculator class which holds the main method
public class Calculator {
    public static void main(String args[]) {
        //create object for calcoolator class
        Calcoolator calc = new Calcoolator();
        //call add method to sum two numbers
        System.out.println("Sum will be: " + calc.add(10,20));
        //call add method to subtract two numbers
        System.out.println("Subtraction will be: " + calc.sub(10,20));
        //call add method to sum two numbers
        System.out.println("Division will be: " + calc.div(10,20));
        //call add method to sum two numbers
        System.out.println("Multiplication will be: " + calc.mul(10,20));
 }
}
