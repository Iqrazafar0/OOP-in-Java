//create class objectpass
class ObjectPass {
    //Attributes
    public int value;
    //define method 'increment'
    public static void increment(ObjectPass a) {
    a.value++; }
    }
    //OBJ == ObjectPassTest
    public class OBJ {
    public static void main(String[] args) {
        //Create object 'p' for objectpass class
    ObjectPass p = new ObjectPass();
    //define value to 'p'
    p.value = 5;
    System.out.println("Before calling: " + p.value); // output is 5
    //using method for pass increment
    ObjectPass.increment(p);
    System.out.println("After calling: " + p.value); // output is 6
    }
    }
